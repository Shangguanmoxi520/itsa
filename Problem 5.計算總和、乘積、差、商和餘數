#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
    int a, b;
    scanf("%d %d", &a, &b);
    printf("%d+%d=%d\n", a, b, a + b);
    printf("%d*%d=%d\n", a, b, a * b);
    printf("%d-%d=%d\n", a, b, a - b);
    if(a >= 0 || a%b == 0)
        printf("%d/%d=%d...%d\n", a, b, a/b, a%b);
    else
        printf("%d/%d=%d...%d\n", a, b, a/b - 1, b +a%b);
        // negative case
	return 0;
}
