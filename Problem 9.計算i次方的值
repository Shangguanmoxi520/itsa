#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
    int n, m;
    scanf("%d", &n);
    int k;
    for(int i = 0; i < n; i++){
        k = 1;
        scanf("%d", &m);
        if(m>31){
            printf("Value of more than 31\n");
            continue;
        }
        printf("%d\n", k<<m);
    }
	return 0;
}
